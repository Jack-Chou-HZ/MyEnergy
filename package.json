{
  "name": "MyEnergy",
  "version": "1.0.0",
  "description": "The MyEnergy web app.",
  "main": "index.js",
  "repository": "git@github.com:Jack-Chou-HZ/MyEnergy.git",
  "author": "Jack-Chou-HZ <better.product.go@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "transf": "babel ./src/views/*.jsx -d ./src/views --config-file ./.babelrc",
    "lint": "eslint ./src{,/**}/*.{js,jsx} --fix --quiet",
    "make-dev": "yarn transf && yarn lint",
    "downgrade": "babel src -d ./downgraded --config-file ./.babelrc.downgrade",
    "compress": "node node_modules/requirejs/bin/r.js -o ./r.compress.config.js",
    "make-release": "yarn make-dev && yarn downgrade && yarn compress"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-export": "^0.1.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.0",
    "prop-types": "^15.7.2",
    "requirejs": "^2.3.6"
  },
  "packageManager": "yarn@3.0.2"
}
